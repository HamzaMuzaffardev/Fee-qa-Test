      gsap.to(".second-section-{{ section.id }} header .h1-high-opacity", {
        width: "100%",
        duration: 4,
        ease: "power2.out",
        scrollTrigger: {
          trigger: ".second-section-{{ section.id }} header h1",
          start: "top 80%",
          once: true
        }
      });
      
      // Optional: Add animation for the button
      gsap.from(".second-section-{{ section.id }} .button-{{ section.id }}", {
        y: 50,
        opacity: 0,
        duration: 1,
        delay: 0.5,
        ease: "power2.out",
        scrollTrigger: {
          trigger: ".second-section-{{ section.id }} .buttons",
          start: "top 85%",
          once: true
        }
      });
    }
  });
</script>

