
{% style %}
  .button-{{ section.id }} {
    width: 322px;
    height: 94px;
    border: 1px solid {{ section.settings.button_border_color }};
    box-sizing: border-box;
    background-color: transparent;
    padding: 10px 20px;
    color: {{ section.settings.button_text_color }};
    font-size: 15px;
    font-weight: 300;
    border-radius: 500px;
    text-transform: uppercase;
    cursor: pointer;
    z-index: 10;
    transition: all 0.3s ease;
  }

  .button-{{ section.id }}:hover {
    background-color: {{ section.settings.button_border_color }};
    color: {{ section.settings.button_hover_text_color }};
  }

  .second-section-{{ section.id }} {
    padding: 80px 0;
    width: 100%;
    background-color: {{ section.settings.background_color }};
    position: relative;
  }

  .second-section-{{ section.id }} header {
    position: absolute;
    width: 100%;
    right: 0;
    top: {{ section.settings.header_position }}px;
  }

  .second-section-{{ section.id }} header h1 {
    margin: -50px 0;
    padding: 0;
    position: absolute;
    right: 90px !important;
    font-family: Cairo, sans-serif;
    color: transparent;
    font-size: 174px !important;
    text-align: right;
    overflow: hidden;
  }

  .second-section-{{ section.id }} header .h1-low-opacity {
    color: transparent;
    background-color: {{ section.settings.low_opacity_color }};
    background-clip: text;
    -webkit-background-clip: text;
  }

  .second-section-{{ section.id }} header .h1-high-opacity {
    color: transparent;
    background-color: {{ section.settings.high_opacity_color }};
    background-clip: text;
    -webkit-background-clip: text;
    display: inline-block;
    width: 0;
    overflow: hidden;
    white-space: nowrap;
  }

  .second-section-{{ section.id }} .background {
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    position: relative;
  }

  .second-section-{{ section.id }} .buttons {
    display: flex;
    flex-direction: row;
    justify-content: center;
    align-items: center;
    gap: 70px;
  }

  @media (max-width: 850px) {
    .button-{{ section.id }} {
      width: 142px;
      height: 60px;
    }
  }

  @media (max-width: 750px) {
    .second-section-{{ section.id }} .buttons {
      gap: 20px;
    }
  }

  @media (max-width: 550px) {
    .button-{{ section.id }} {
      width: 122px;
      height: 54px;
      font-size: 12px;
    }
  }

  @media (max-width: 450px) {
    .second-section-{{ section.id }} {
      padding: 40px 0;
    }

    .second-section-{{ section.id }} .buttons {
      flex-direction: column;
    }

    .second-section-{{ section.id }} header h1 {
      margin: 0;
    }
  }
{% endstyle %}

